{
    "name" : "eddiejaoude/phpunit-wrapper",
    "description" : "PHPUnit wrapper - wraps the functionality of phpunit with easy to use plain English",
    "type" : "library",
    "authors" : [{
        "name" : "Eddie Jaoude",
        "email" : "eddie@jaoudestudios.com"
    }
    ],
    "license": "MIT",
    "keywords" : [
        "PHPUnit",
        "Wrapper"
    ],
    "require" : {
        "php" : ">=5.5",
        "phpunit/phpunit": "3.7.*"
    },
    "require-dev" : {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload" : {
        "psr-4" : {
            "EddieJaoude\\PHPUnitWrapper\\" : "src/PHPUnitWrapper"
        }
    }
}
